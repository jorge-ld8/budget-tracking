  "scripts": {
    "start": "NODE_ENV=production node server.ts",
    "dev": "NODE_ENV=development nodemon server.ts",
    "dev:ts": "NODE_ENV=development ts-node --transpile-only server.ts",
    "test": "NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules npx jest --config=jest.config.js",
    "test:watch": "NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules npx jest --config=jest.config.js --watch",
    "test:coverage": "NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules npx jest --config=jest.config.js --coverage",
    "test:specific": "NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules npx jest --config=jest.config.js --testPathPattern",
    "migration:create": "migrate-mongo create",
    "migration:up": "migrate-mongo up",
    "migration:down": "migrate-mongo down",
    "migration:status": "migrate-mongo status",
    "migration:create:dev": "NODE_ENV=development migrate-mongo create",
    "migration:up:dev": "NODE_ENV=development migrate-mongo up",
    "migration:down:dev": "NODE_ENV=development migrate-mongo down",
    "migration:status:dev": "NODE_ENV=development migrate-mongo status"
  },
